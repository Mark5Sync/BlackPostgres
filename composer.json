            "blackpostgres\\run::scripts_command"
        ],
        "bd:generate": "blackpostgres\\run::generate",
        "scr": "blackpostgres\\run::scripts_command"
    },
    "bin": [
        "bin\/mark-orm-build-scheme"
    ]
