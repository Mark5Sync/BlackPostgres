{
    "name": "blackpostgres\/blackpostgres",
    "description": "black postgres",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "blackpostgres\\": "src\/"
        }
    },
    "require": {
        "marksync\/provider": "^1.0",
        "composer\/class-map-generator": "^1.1",
        "illuminate\/database": "^11.7",
        "twcli\/twcli": "^1.0",
        "marksync/useconfig": "^0.0.3",
        "laravel/serializable-closure": "^1.3"
    },
    "scripts": {
        "mark": "php vendor\/bin\/mark",
        "db:generate": "blackpostgres\\run::generate",
        "test": "./vendor/bin/pest"
    },
    "require-dev": {
        "pestphp/pest": "^2.34"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
